<odoo>
  <record id="spqm_product_entry_action" model="ir.actions.act_window">
    <field name="name">Product Entry</field>
    <field name="res_model">spqm.product_entry</field>
    <field name="view_mode">list,form</field>
  </record>

  <record id="spqm_product_entry_view_tree" model="ir.ui.view">
    <field name="name">spqm.product_entry.tree</field>
    <field name="model">spqm.product_entry</field>
    <field name="arch" type="xml">
      <tree>
        <field name="product_id" />
        <field name="quantity" />
      </tree>
    </field>
  </record>

  <record id="spqm_product_entry_view_form" model="ir.ui.view">
    <field name="name">spqm.product_entry.form</field>
    <field name="model">spqm.product_entry</field>
    <field name="arch" type="xml">
      <form>
        <group>
          <field name="product_id" />
          <field name="quantity" />
          <field name="vat" />
        </group>
      </form>
    </field>
  </record>
</odoo>
